	</bean>
	<bean id="segment9de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="${segment9de}">
		<property name="segment" ref="segment9" />
	</bean>
	<bean id="segment10de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="${segment10de}">
		<property name="segment" ref="segment10" />
	</bean>
	<bean id="segment11de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="${segment11de}">
		<property name="segment" ref="segment11" />
	</bean>
	<bean id="segment12de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="${segment12de}">
