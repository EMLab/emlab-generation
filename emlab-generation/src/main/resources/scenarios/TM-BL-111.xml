				p:segmentID="12" />
			<bean id="segment10" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="576"
				p:segmentID="11" />
			<bean id="segment11" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="576"
				p:segmentID="10" />
			<bean id="segment12" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="576"
