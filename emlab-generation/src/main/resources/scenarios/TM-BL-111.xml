			<bean id="segment15" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="576"
				p:segmentID="6" />
			<bean id="segment16" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="576"
