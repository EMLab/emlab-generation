			<bean id="segment17" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="576"
				p:segmentID="4" />
			<bean id="segment18" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="50"
				p:segmentID="3" />
			<bean id="segment19" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="9"
				p:segmentID="2" />
			<bean id="segment20" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="1"
				p:segmentID="1" />

	<!-- Markets -->
	<!-- Electricity Spot -->
	<bean id="germanElectricitySpotMarket" class="emlab.gen.domain.market.electricity.ElectricitySpotMarket"
		p:name="Country A electricity spot market" p:auction="false"
		p:referencePrice="40" p:valueOfLostLoad="5000" p:lookback="5" p:backlookingForDemandForecastinginDismantling="5">
		<property name="substance" ref="electricity" />
		<property name="demandGrowthTrend" ref="demandGrowthTrendDE" />
		<property name="zone" ref="de" />

		<property name="loadDurationCurve">
			<set>
				<ref bean="segment1de" />
				<ref bean="segment2de" />
				<ref bean="segment3de" />
				<ref bean="segment4de" />
				<ref bean="segment5de" />
				<ref bean="segment6de" />
				<ref bean="segment7de" />
				<ref bean="segment8de" />
				<ref bean="segment9de" />
				<ref bean="segment10de" />
				<ref bean="segment11de" />
				<ref bean="segment12de" />
				<ref bean="segment13de" />
				<ref bean="segment14de" />
				<ref bean="segment15de" />
				<ref bean="segment16de" />
				<ref bean="segment17de" />
				<ref bean="segment18de" />
				<ref bean="segment19de" />
				<ref bean="segment20de" />
			</set>

		</property>

	</bean>

	<bean id="segment1de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="${segment1de}">
		<property name="segment" ref="segment1" />
	</bean>
	<bean id="segment2de" class="emlab.gen.domain.market.electricity.SegmentLoad"
